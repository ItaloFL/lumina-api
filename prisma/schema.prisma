generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum AuthProvider {
  LOCAL
  GOOGLE
  GITHUB
}

model User {
  id          String       @id @default(uuid())
  image_url   String?
  name        String
  email       String       @unique
  password    String?
  dateOfBirth DateTime?
  provider    AuthProvider @default(LOCAL)
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
}
